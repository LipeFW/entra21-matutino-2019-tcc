<div class="modal" tabindex="-1" role="dialog" id="modal-inventario">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">@View.Resources.Resource.CadastrodeInventario</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
              
                <div class="form-group">
                    <label for="inventario-campo-numerocaminhao">@View.Resources.Resource.NumeroCaminhao</label>
                    <input type="text" id="inventario-campo-numerocaminhao" class="form-control" />
                </div>
               


            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-danger" data-dismiss="modal"><i class="fas fa-times"></i> @View.Resources.Resource.Cancelar</button>
                <button id="inventario-botao-salvar" type="button" class="btn btn-success"><i class="fas fa-save"></i>  @View.Resources.Resource.Salvar</button>
            </div>
        </div>
    </div>
</div>
@section js{

    <script src="~/Scripts/Mascaras/Mask.js"></script>
}


