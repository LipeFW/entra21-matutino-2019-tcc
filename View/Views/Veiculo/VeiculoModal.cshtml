<div class="modal" tabindex="-1" role="dialog" id="modal-veiculo">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Cadastro de Veículo</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label for="veiculo-campo-modelo">Modelo</label>
                    <input id="veiculo-campo-modelo" class="form-control" />
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-danger" data-dismiss="modal">Fechar</button>
                <button type="button" class="btn btn-success" id="veiculo-botao-salvar">Salvar</button>
            </div>
        </div>
    </div>
</div>
