@{
    ViewBag.Title = "Cliente";
}
<link rel="stylesheet" type="text/css" href="~/Content/css/tabela/util.css" />
<h3 class="text-center mb-3 mt-3 text-info invisible">Lista de Clientes</h3>
<div>

    <h2 class="text-center">Clientes</h2>
    <table id="cliente-tabela" class=" mr-0 ml-0 table table-bordered table-hover table-striped">
        <thead>
            <tr>
                <th>Código</th>
                <th>Nome</th>
                <th>Telefone</th>
                <th>CNPJ</th>
                <th>CPF</th>
                <th>CEP</th>
                <th>Vendedor</th>
                <th>Ação</th>
            </tr>
        </thead>
    </table>
</div>

@section js{
    <script src="~/Scripts/Cliente/cliente.js">
    </script>
}


