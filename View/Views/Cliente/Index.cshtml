@{
    ViewBag.Title = "Cliente";
}

<h3 class="text-center mb-3 mt-3 text-info invisible">lista de clientes</h3>
<div>

    <h2 class="text-center">Clientes</h2>
    <table id="cliente-tabela" class="table table-bordered table-hover table-striped">
        <thead>
            <tr>
                <th>Código</th>
                <th>Nome</th>
                <th>Telefone</th>
                <th>CNPJ</th>
                <th>CEP</th>
                <th>Vendedor</th>
                <th>Ação</th>
            </tr>
        </thead>
    </table>
</div>

@section js{
    <script src="~/Scripts/Cliente/cliente.js">
    </script>
}


