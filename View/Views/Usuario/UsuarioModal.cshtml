<div class="modal bounceInDown animated mt-6" tabindex="-1" role="dialog" id="modal-usuario">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title"><i class="fas fa-user"></i>@View.Resources.Resource.CadastrodeUsuario</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body back">
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-nome1">@View.Resources.Resource.Nome <span class="text-danger">*</span></label>
                    <input id="usuario-campo-nome1" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-sobrenome">@View.Resources.Resource.Sobrenome <span class="text-danger">*</span></label>
                    <input id="usuario-campo-sobrenome" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-nome">@View.Resources.Resource.NomeUsuario <span class="text-danger">*</span></label>
                    <input id="usuario-campo-nome" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-senha">@View.Resources.Resource.Senha <span class="text-danger">*</span></label>
                    <input type="password" id="usuario-campo-senha" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-telefone">@View.Resources.Resource.Telefone <span class="text-danger">*</span></label>
                    <input id="usuario-campo-telefone" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-cpf">@View.Resources.Resource.CPF <span class="text-danger">*</span></label>
                    <input id="usuario-campo-cpf" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-rg">@View.Resources.Resource.RG <span class="text-danger">*</span></label>
                    <input id="usuario-campo-rg" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-cep">@View.Resources.Resource.CEP <span class="text-danger">*</span></label>
                    <input id="usuario-campo-cep" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="config-campo-pais">@View.Resources.Resource.Pais <span class="text-danger">*</span></label>
                    <br />
                    <select class="form-control border-dark" style="width: 100%; " id="config-campo-pais">
                        <option disabled="disabled" selected="selected"> @View.Resources.Resource.Selecioneumpaís</option>
                    </select>
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="config-campo-estado">@View.Resources.Resource.Estado <span class="text-danger">*</span></label>
                    <br>
                    <select class="form-control border-dark" style="width: 100% " id="config-campo-estado">
                        <option disabled="disabled" selected="selected"> @View.Resources.Resource.Selecioneumestado</option>

                    </select>
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="config-campo-cidade">@View.Resources.Resource.Cidade <span class="text-danger">*</span></label>
                    <br>
                    <select class="form-control border-dark" style="width: 100% " id="config-campo-cidade">
                        <option disabled="disabled" selected="selected"> @View.Resources.Resource.Selecioneumacidade</option>
                    </select>
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-logradouro">@View.Resources.Resource.Logradouro <span class="text-danger">*</span></label>
                    <input id="usuario-campo-logradouro" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <label style="font-weight:bold" for="usuario-campo-numerocasa">@View.Resources.Resource.NumeroCasa <span class="text-danger">*</span></label>
                    <input id="usuario-campo-numerocasa" class="form-control border-dark" />
                </div>
                <div class="form-group">
                    <input id="usuario-campo-imagem" type="hidden" name="UrlImagem" value="default" />
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="rubberBand animated btn btn-danger" data-dismiss="modal"><i class="fas fa-times"></i> @View.Resources.Resource.Cancelar</button>
                <button id="alterarSenha" type="button" class="rubberBand animated btn btn-success"><i class="fa fa-check"></i>  @View.Resources.Resource.Salvar</button>
            </div>
        </div>
    </div>
</div>